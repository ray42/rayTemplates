doi = {10.1021/bi800678t},
file = {:home/ray/mendeley{\_}sorted/Psachoulia et al. - 2008 - Helix-helix interactions in membrane proteins Coarse-grained simulations of glycophorin A helix dimerization.pdf:pdf},
